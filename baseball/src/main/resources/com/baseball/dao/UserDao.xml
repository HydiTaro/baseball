<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.baseball.dao.UserDao">
       <!-- idには対応するメソッド名を記述します -->
    <!-- select処理なのでselectタグで囲います -->
    <select id="findByName" resultType="com.baseball.security.UserForm">
        SELECT * 
        FROM login_user 
        WHERE user_id = #{conditionEntity}
    </select>
</mapper>