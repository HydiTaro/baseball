<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
        
<mapper namespace="com.baseball.dao.SecurityServiceDao">
   <select id="getLoginUserByEmail" parameterType="java.util.List">
        <!--試合データがテーブルへ登録していない場合、挿入処理を行う-->
        SELECT
            *
        FROM
            login_user
        WHERE
            email = #{conditionEntity}    
    </select>
</mapper>